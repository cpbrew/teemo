{
  "preset": "airbnb",
  "disallowMultipleVarDecl": null,
  "disallowKeywordsOnNewLine": ["else", "catch"],
  "disallowTrailingWhitespace": "ignoreEmptyLines",
  "disallowQuotedKeysInObjects": "allButReserved",
  "disallowTrailingWhitespace": "ignoreEmptyLines",
  "requireCurlyBraces": [
    "if",
    "else",
    "for",
    "while",
    "do",
    "try",
    "catch"
  ],
  "esnext": true,
  "requirePaddingNewLinesAfterBlocks": null
}
